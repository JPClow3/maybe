{
	"devDependencies": {
		"@biomejs/biome": "^1.9.3",
		"@lhci/cli": "^0.12.0",
		"tailwindcss": "^3.4.1",
		"postcss": "^8.4.35",
		"autoprefixer": "^10.4.17"
	},
	"name": "maybe-django",
	"version": "2.0.0",
	"description": "Personal finance app built with Django",
	"scripts": {
		"style:check": "biome check",
		"style:fix": "biome check --write",
		"lint": "biome lint",
		"lint:fix": "biome lint --write",
		"format:check": "biome format",
		"format": "biome format --write",
		"css:build": "tailwindcss -i static/css/input.css -o static/css/main.css --minify",
		"css:watch": "tailwindcss -i static/css/input.css -o static/css/main.css --watch",
		"postinstall": "npm run css:build",
		"lighthouse:ci": "lhci autorun",
		"lighthouse:audit": "lhci autorun --collect.url=http://localhost:8000"
	},
	"author": "",
	"license": "AGPL-3.0"
}
